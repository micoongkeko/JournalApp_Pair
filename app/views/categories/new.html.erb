<%= form_with model: @category do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <div>
    <% @category.errors.full_messages_for(:title).each do |message| %>
      <div class="form-text form-text-validation"><%= message %></div>
    <% end %>
    </div>
    <%= f.number_field :user_id, id: :category_user_id, value: current_user.id, type: :hidden %>
  </div>
  <%= f.submit 'Create Category' %>
<% end %>